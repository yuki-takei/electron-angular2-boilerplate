{
  "name": "electron-angular2-boilerplate",
  "version": "1.0.0",
  "description": "Electron app boilerplate using gulp, jspm, and Angular 2",
  "author": "Yuki Takei <yuki@weseek.co.jp>",
  "license": "Apache-2.0",
  "keywords": [
    "angular2",
    "babel",
    "dtsm",
    "electron",
    "es6",
    "jspm",
    "less",
    "typescript"
  ],
  "scripts": {
	  "update": "npm install && jspm install"
  },
  "dependencies": {
    "log4js": "~0.6.0"
  },
  "devDependencies": {
    "babel": "~5.8.0",
    "browserify": "~11.0.0",
    "del": "~1.2.0",
    "electron-connect": "~0.2.0",
    "electron-packager": "~5.0.0",
    "gulp": "~3.9.0",
    "gulp-babel": "~5.2.0",
    "gulp-cssmin": "~0.1.0",
    "gulp-env": "~0.2.0",
    "gulp-filter": "~3.0.0",
    "gulp-less": "~3.0.0",
    "gulp-load-plugins": "~0.10.0",
    "gulp-minify-html": "~1.0.0",
    "gulp-plumber": "~1.0.0",
    "gulp-shell": "^0.4.2",
    "gulp-sourcemaps": "~1.5.0",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "~3.0.0",
    "gulp-watch": "~4.3.0",
    "gulp-webserver": "~0.9.0",
    "jspm": "~0.16.0",
    "lodash": "^3.10.0",
    "merge2": "~0.3.0",
    "path": "~0.11.0",
    "run-sequence": "~1.1.0",
    "uglify-save-license": "~0.4.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "wrench": "~1.5.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "configFile": "src/jspm.config.js",
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-alpha.35",
      "bootstrap": "github:twbs/bootstrap@^3.3.5",
      "es6-shim": "github:es-shims/es6-shim@^0.33.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.0",
      "zone.js": "npm:zone.js@^0.5.2"
    },
    "devDependencies": {
      "clean-css": "npm:clean-css@^3.3.9",
      "css": "github:systemjs/plugin-css@^0.1.13",
      "less": "github:aaike/jspm-less-plugin@^0.0.5",
      "typescript": "npm:typescript@^1.6.0-dev.20150821"
    }
  }
}
